<?xml version="1.0" encoding="utf-8"?>
<com.snapdonate.app.ui.SampleAppMenu.SampleAppMenuView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0000"
    >
    
	<ScrollView
	    android:background="#FFF"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    >
	          
	    <LinearLayout 
	    	android:id="@+id/settingsMenuLinearLayout" 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="#0000"
		    android:orientation="vertical"
	        >
	        
		    <TextView 
			    android:id="@+id/settingsMenuTitleTextView" 
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:gravity="center_horizontal"
			    android:background="@color/light_gray"
			    android:padding="@dimen/menu_title_padding"
			    android:textStyle="bold"
			    android:textSize="@dimen/menu_title"
		        />
		    
	     </LinearLayout>
	</ScrollView>
	
</com.snapdonate.app.ui.SampleAppMenu.SampleAppMenuView><!-- From: file:/E:/Android%20Studio%20Workspace/snap-donate-andriod-github/snapDonate/src/main/res/layout/snap_donate_app_menu_layer.xml -->